{
  "name": "teampa",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "build": "tsc --watch",
    "start": "node dist/app.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/app.ts",
    "prepare": "husky",
    "lint": "eslint .",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "format": "prettier --write",
    "format:file": "prettier --write",
    "db:migrate": "sequelize-cli db:migrate --require ts-node/register",
    "db:migrate:undo": "sequelize-cli db:migrate:undo --require ts-node/register",
    "db:seed": "sequelize-cli db:seed:all --require ts-node/register",
    "db:seed:undo": "sequelize-cli db:seed:undo:all --require ts-node/register",
    "db:migration:generate": "sequelize-cli migration:generate --name",
    "db:seed:generate": "sequelize-cli seed:generate --name"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/migrate-config": "^1.5.2",
    "@types/app-root-path": "^3.1.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "dotenv": "^17.0.1",
    "eslint": "^9.30.1",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "install": "^0.13.0",
    "jest": "^30.0.4",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "npm": "^11.4.2",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "compression": "^1.8.0",
    "express-winston": "^4.2.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
